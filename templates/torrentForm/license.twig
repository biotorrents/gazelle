{# license #}
<tr>
	<th>
		<label for="license" class="required">License</label>
	</th>

	<td>
		<select id="license" name="license" class="select">
			<option value="">{{ db.description }}</option>
			{% for item in env.metadata.licenses %}
				<option value="{{ item }}">{{ item }}</option>
			{% endfor %}
		</select>

		<aside>
			{{ db.note|raw }}
		</aside>
	</td>
</tr>
