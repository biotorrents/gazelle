<div id="starboardNotebook"></div>

<script type="module">
    import {StarboardEmbed} from "/js/vendor/starboard-wrap.js"
    const mount = document.querySelector("#starboardNotebook");

    const el = new StarboardEmbed({
        notebookContent: "{{ content|raw }}",
        src: "https://starboard.torrents.bio"
    });

    mount.appendChild(el);
</script>
