{
  "name": "biotorrents/gazelle",
  "description": "An open platform for libre biology data",
  "license": "ISC",
  "type": "project",
  "keywords": [
    "biological-data",
    "bittorrent-tracker"
  ],
  "readme": "README.md",
  "authors": [
    {
      "name": "Omics Tools LLC",
      "email": "hello@torrents.bio"
    }
  ],
  "homepage": "https://torrents.bio",
  "support": {
    "email": "hello@torrents.bio",
    "issues": "https://github.com/biotorrents/gazelle/issues",
    "forum": "https://torrents.bio/boards",
    "wiki": "https://torrents.bio/wiki",
    "chat": "https://slack.torrents.bio",
    "source": "https://github.com/biotorrents/gazelle",
    "docs": "https://docs.torrents.bio"
  },
  "funding": [
    {
      "type": "patreon",
      "url": "https://www.patreon.com/biotorrents"
    },
    {
      "type": "other",
      "url": "https://torrents.bio/donate"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-apcu": "*",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-mbstring": "*",
    "ext-mysqli": "*",
    "ext-pdo": "*",
    "ext-redis": "*",
    "ext-zip": "*",
    "abraham/twitteroauth": "^4.0.1",
    "cartalyst/sentinel": "^6.0",
    "delight-im/auth": "^8.3",
    "erusev/parsedown": "1.8.0-beta-7",
    "erusev/parsedown-extra": "0.8.0",
    "foolz/sphinxql-query-builder": "^3.0.2",
    "guzzlehttp/guzzle": "^7.6.1",
    "illuminate/database": "^9.52.7",
    "illuminate/support": "^9.52.7",
    "mikecao/flight": "^2.0.1",
    "nesbot/carbon": "^2.66",
    "openai-php/client": "^0.2.1",
    "orpheusnet/bencode-torrent": "^1.2",
    "p3k/emoji-detector": "^1.0",
    "paragonie/anti-csrf": "^2.3",
    "phpmailer/phpmailer": "^6.8",
    "povils/figlet": "*",
    "ramsey/uuid": "^4.7.4",
    "robthree/twofactorauth": "^1.8.2",
    "snipe/banbuilder": "^2.3",
    "sofa/eloquence": "dev-master",
    "spatie/once": "^3.1",
    "twig/twig": "^3.6.0",
    "vanilla/nbbc": "^2.4",
    "web-auth/webauthn-lib": "^4.5.2"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.31.0",
    "friendsofphp/php-cs-fixer": "^3.23",
    "kint-php/kint": "^4.2.3",
    "maximebf/debugbar": "^1.18.2",
    "phplucidframe/console-table": "^1.2.4",
    "phpunit/phpunit": "^9.6.8",
    "psy/psysh": "^0.11.17",
    "robmorgan/phinx": "^0.13.4"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/",
      "Gazelle\\": "app/"
    },
    "classmap": [
      "app/"
    ],
    "files": [
      "bootstrap/autoload.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    },
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
